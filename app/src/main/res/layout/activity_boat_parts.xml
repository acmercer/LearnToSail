<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/ic_background"
    tools:context=".BoatParts">

    <ImageView
        android:id="@+id/imageView13"
        android:layout_width="0dp"
        android:layout_height="151dp"
        android:layout_marginStart="256dp"
        android:layout_marginBottom="608dp"
        android:contentDescription="@string/logodesc"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/choice_1"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/ic_logosmall" />

    <ImageButton
        android:id="@+id/btnBoatBack"
        android:layout_width="92dp"
        android:layout_height="92dp"
        android:layout_marginEnd="168dp"
        android:layout_marginBottom="48dp"
        android:adjustViewBounds="true"
        android:backgroundTint="#00FFFFFF"
        android:contentDescription="@string/design"
        android:scaleType="fitCenter"
        android:scrollbarThumbHorizontal="@drawable/ic_backbutton"
        app:layout_constraintBottom_toBottomOf="@+id/imageView13"
        app:layout_constraintEnd_toStartOf="@+id/imageView13"
        app:srcCompat="@drawable/ic_baseline_navigate_before_24"
        app:tint="@color/black"
        tools:ignore="DuplicateSpeakableTextCheck" />

    <ImageButton
        android:id="@+id/btnBoatHome"
        style="@android:style/Widget.ImageButton"
        android:layout_width="60dp"
        android:layout_height="0dp"
        android:layout_marginEnd="124dp"
        android:layout_marginBottom="124dp"
        android:background="#003C2727"
        android:contentDescription="@string/homebtn"
        android:scaleType="fitCenter"
        app:layout_constraintBottom_toTopOf="@+id/mainBlank"
        app:layout_constraintEnd_toStartOf="@+id/imageView13"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/ic_baseline_home_24"
        app:tint="#000000" />

    <ImageView
        android:id="@+id/boatDesign"
        android:layout_width="381dp"
        android:layout_height="538dp"
        android:contentDescription="@string/boatparts"
        android:scaleType="fitCenter"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.466"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.31"
        app:srcCompat="@drawable/boat" />

    <TextView
        android:id="@+id/choice_1"
        android:layout_width="138dp"
        android:layout_height="24dp"
        android:layout_marginStart="132dp"
        android:layout_marginTop="60dp"
        android:background="@drawable/blanklabel"
        android:gravity="center"
        android:text=""
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnBoatHome" />

    <TextView
        android:id="@+id/mainBlank"
        android:layout_width="138dp"
        android:layout_height="24dp"
        android:layout_marginTop="308dp"
        android:layout_marginEnd="32dp"
        android:background="@drawable/blanklabel"
        android:gravity="center"
        android:text=""
        app:layout_constraintEnd_toStartOf="@+id/imageView13"
        app:layout_constraintTop_toBottomOf="@+id/btnBoatHome" />

    <TextView
        android:id="@+id/mastBlank"
        android:layout_width="138dp"
        android:layout_height="24dp"
        android:layout_marginTop="64dp"
        android:layout_marginEnd="28dp"
        android:background="@drawable/blanklabel"
        android:gravity="center"
        android:text=""
        app:layout_constraintEnd_toEndOf="@+id/boatDesign"
        app:layout_constraintTop_toTopOf="@+id/boatDesign" />

    <TextView
        android:id="@+id/jibBlank"
        android:layout_width="138dp"
        android:layout_height="24dp"
        android:layout_marginTop="304dp"
        android:layout_marginEnd="12dp"
        android:background="@drawable/blanklabel"
        android:gravity="center"
        android:text=""
        app:layout_constraintEnd_toEndOf="@+id/boatDesign"
        app:layout_constraintTop_toTopOf="@+id/boatDesign" />

    <TextView
        android:id="@+id/hullBlank"
        android:layout_width="138dp"
        android:layout_height="24dp"
        android:layout_marginTop="444dp"
        android:layout_marginEnd="168dp"
        android:background="@drawable/blanklabel"
        android:gravity="center"
        android:text=""
        app:layout_constraintEnd_toEndOf="@+id/boatDesign"
        app:layout_constraintTop_toTopOf="@+id/boatDesign" />

    <TextView
        android:id="@+id/daggerBlank"
        android:layout_width="138dp"
        android:layout_height="24dp"
        android:layout_marginTop="512dp"
        android:layout_marginEnd="28dp"
        android:background="@drawable/blanklabel"
        android:gravity="center"
        android:text=""
        app:layout_constraintEnd_toEndOf="@+id/boatDesign"
        app:layout_constraintTop_toTopOf="@+id/boatDesign" />

    <TextView
        android:id="@+id/kickerBlank"
        android:layout_width="138dp"
        android:layout_height="24dp"
        android:layout_marginStart="4dp"
        android:layout_marginTop="96dp"
        android:background="@drawable/blanklabel"
        android:gravity="center"
        android:text=""
        app:layout_constraintStart_toEndOf="@+id/orangeRope"
        app:layout_constraintTop_toTopOf="@+id/boatDesign" />

    <TextView
        android:id="@+id/purpleRope"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="128dp"
        android:layout_marginEnd="1dp"
        android:text="@string/purpleRope"
        android:textSize="20sp"
        app:layout_constraintEnd_toStartOf="@+id/jibsheetBlank"
        app:layout_constraintTop_toTopOf="@+id/boatDesign" />

    <TextView
        android:id="@+id/orangeRope"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="96dp"
        android:layout_marginEnd="5dp"
        android:text="@string/orangerope"
        android:textSize="20sp"
        app:layout_constraintEnd_toStartOf="@+id/kickerBlank"
        app:layout_constraintTop_toTopOf="@+id/boatDesign" />

    <TextView
        android:id="@+id/blueRope"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="24dp"
        android:layout_marginTop="60dp"
        android:text="@string/bluerope"
        android:textSize="20sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/boatDesign" />

    <TextView
        android:id="@+id/mainsheetBlank"
        android:layout_width="138dp"
        android:layout_height="24dp"
        android:layout_marginStart="4dp"
        android:layout_marginTop="64dp"
        android:background="@drawable/blanklabel"
        android:gravity="center"
        android:text=""
        app:layout_constraintStart_toEndOf="@+id/blueRope"
        app:layout_constraintTop_toTopOf="@+id/boatDesign" />

    <TextView
        android:id="@+id/ropes"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="72dp"
        android:layout_marginTop="36dp"
        android:text="@string/ropes"
        android:textColor="#000000"
        android:textSize="20sp"
        app:layout_constraintStart_toStartOf="@+id/boatDesign"
        app:layout_constraintTop_toTopOf="@+id/boatDesign" />

    <TextView
        android:id="@+id/jibsheetBlank"
        android:layout_width="138dp"
        android:layout_height="24dp"
        android:layout_marginTop="128dp"
        android:layout_marginEnd="188dp"
        android:background="@drawable/blanklabel"
        android:gravity="center"
        android:text=""
        app:layout_constraintEnd_toEndOf="@+id/boatDesign"
        app:layout_constraintTop_toTopOf="@+id/boatDesign" />

    <TextView
        android:id="@+id/painterBlank"
        android:layout_width="138dp"
        android:layout_height="24dp"
        android:layout_marginTop="444dp"
        android:background="@drawable/blanklabel"
        android:gravity="center"
        android:text=""
        app:layout_constraintEnd_toEndOf="@+id/boatDesign"
        app:layout_constraintTop_toTopOf="@+id/boatDesign" />

    <TextView
        android:id="@+id/boomBlank"
        android:layout_width="138dp"
        android:layout_height="24dp"
        android:layout_marginTop="356dp"
        android:layout_marginEnd="188dp"
        android:background="@drawable/blanklabel"
        android:gravity="center"
        android:text=""
        app:layout_constraintEnd_toEndOf="@+id/boatDesign"
        app:layout_constraintTop_toTopOf="@+id/boatDesign" />

    <TextView
        android:id="@+id/rudderBlank"
        android:layout_width="138dp"
        android:layout_height="24dp"
        android:layout_marginTop="488dp"
        android:layout_marginEnd="260dp"
        android:background="@drawable/blanklabel"
        android:gravity="center"
        android:text=""
        app:layout_constraintEnd_toEndOf="@+id/boatDesign"
        app:layout_constraintTop_toTopOf="@+id/boatDesign" />

    <TextView
        android:id="@+id/tillerBlank"
        android:layout_width="138dp"
        android:layout_height="24dp"
        android:layout_marginTop="404dp"
        android:layout_marginEnd="204dp"
        android:background="@drawable/blanklabel"
        android:gravity="center"
        android:text=""
        app:layout_constraintEnd_toEndOf="@+id/boatDesign"
        app:layout_constraintTop_toTopOf="@+id/boatDesign" />

</androidx.constraintlayout.widget.ConstraintLayout>